{
	"geode": "4.1.1",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "dasshu.better-gauntlets",
	"name": "Better Gauntlets",
	"version": "v1.1.1",
	"developer": "DasshuDev",
	"description": "Brings the Gauntlets to life!",
	"links": {
		"source": "https://github.com/DasshuDev/BetterGauntlets",
		"community": "https://discord.gg/F94qat6N3D",
		"homepage": "https://dasshu.dev"
	},
	"issues": {
		"info": "https://github.com/DasshuDev/BetterGauntlets/issues"
	},
	"tags": [
		"interface",
		"enhancement"
	],
	"resources": {
		"files": [
			"resources/fonts/*.fnt",
			"resources/fonts/*.png"
		],
		"sprites": [
			"resources/GauntletLayer/**/*.png",
			"resources/GauntletSelectLayer/**/*.png"
		]
	},
	"dependencies": [
        {
            "id": "geode.node-ids",
            "version": ">=v1.15.0",
            "importance": "required"
        },
		{
			"id": "geode.custom-keybinds",
            "version": ">=v1.0.0",
            "importance": "required",
			"platforms": ["win"]
		}
	],
	"settings": {
		"update-discord-sprite": {
			"name": "Update Discord Sprite",
			"description": "Updates the <cj>Discord</c> logo on the <cj>Discord</c> Gauntlet to <cg>match</c> with the current company logo.\n\n<cr>This does not work on GauntletSelectLayer.</c>",
			"type": "bool",
			"default": false
		}
	}
}