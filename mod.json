{
	"geode": "4.2.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "dasshu.better-gauntlets",
	"name": "Better Gauntlets",
	"version": "v1.1.4-beta.2",
	"developer": "DasshuDEV",
	"description": "Brings Gauntlets to life with new visuals!",
	"links": {
		"source": "https://github.com/DasshuDev/BetterGauntlets",
		"community": "https://discord.gg/F94qat6N3D",
		"homepage": "https://dasshu.dev"
	},
	"issues": {
		"info": "https://github.com/DasshuDev/BetterGauntlets/issues"
	},
	"tags": [
		"interface",
		"enhancement"
	],
	"resources": {
		"files": [
			"resources/fonts/*.fnt",
			"resources/fonts/*.png"
		],
		"sprites": [
			"resources/*.png",
			"resources/GauntletSelectLayer/**/*.png"
		],
		"spritesheets": {
			"GauntletIconSheet_001": [
				"resources/GauntletLayer/levelSprites/*.png"
			],
			"GauntletUISheet_001":[
				"resources/GauntletLayer/UI/*.png",
				"resources/GauntletSelectLayer/*.png"
			]
		}
	},
	"dependencies": {
		"geode.node-ids": ">=v1.19.0",
		"geode.custom-keybinds": {
			"importance": "required",
			"version": ">=v1.0.0",
			"platforms": [
				"win"
			]
		}
	},
	"settings": {
		"experimental": {
			"name": "Experimental",
			"type": "title"
		},
		"update-discord-sprite": {
			"name": "Update Discord Sprite",
			"description": "Updates the <cj>Discord</c> logo on the <cj>Discord</c> Gauntlet to match with the <cg>current</c> company logo.\n\n<cr>This does not work on GauntletSelectLayer.</c>",
			"type": "bool",
			"default": false
		}
	}
}
